      x = (x * x) % mod;
      pow >>= 1;
   }
   return ret;
}
#define INVERSE(a, m) bigmod(a, m-2, m)
int gauss(long long a[MAX][MAX], long long ans[MAX], int n, int m, long long mod)
{
