*/

int x[MAX],n;
double a[MAX][MAX],ans[MAX];

int main()
{
    //freopen("input.txt","r",stdin);

    int t,i,j;
    si(t);
    for(int cs=1;cs<=t;cs++){
        for(i=0;i<=100;i++)x[i]=i;
        si(n);
        for(i=1;i<=n;i++){
            int u,v;
            si(u);si(v);
            u--;v--;
            x[u]=v;
        }
        for(i=0;i<MAX;i++)
            for(j=0;j<MAX;j++)a[i][j]=0.0;

        for(i=0;i<99;i++){
            if(x[i]!=i)continue;
            a[i][i]=1.0;
            for(j=1;j<=6;j++){
                if(i+j<=99){
                    a[i][x[i+j]]-=(1.0/6.0);
                    a[i][100]+=(1.0/6.0);
                }
                else{
                    a[i][i]-=(1.0/6.0);
                    a[i][100]+=(1.0/6.0);
                }
            }
        }
        a[99][99]=1.0;
        gauss(a,ans,100,100);
        printf("Case %d: %.10f\n",cs,ans[0]);
    }

}
